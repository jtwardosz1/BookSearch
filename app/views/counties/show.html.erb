<p style="color: green"><%= notice %></p>

<%= render @county %>


<!-- added @ sign to county here after consulting with chatgpt regarding error -->
<p>
 <strong>Libraries:</strong><br>
  <% @county.libraries.each do |library| %>
    <%= library.name %>
    <br>
  <% end %>
</p>
  

<div>
  <%= link_to "Edit this county", edit_county_path(@county) %> |
  <%= link_to "Back to counties", counties_path %>

  <%= button_to "Destroy this county", @county, method: :delete %>
</div>


